<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- control block base template -->

    <ControlTemplate x:Key="ControlBlockBaseTemplate">
        <Canvas Width="16.0cm" Height="4.0cm" ClipToBounds="False">
            <!-- horizontal lines -->
            <Line X1="0.0cm" Y1="0.0cm" X2="16.0cm" Y2="0.0cm" Style="{StaticResource ResourceKey=ControlBlockBorderLineStyle}"/>
            <Line X1="0.0cm" Y1="0.5cm" X2="16.0cm" Y2="0.5cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="0.0cm" Y1="1.0cm" X2="16.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="0.0cm" Y1="1.5cm" X2="1.0cm" Y2="1.5cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="0.0cm" Y1="2.0cm" X2="1.0cm" Y2="2.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="0.0cm" Y1="2.5cm" X2="1.0cm" Y2="2.5cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="15.0cm" Y1="1.5cm" X2="16.0cm" Y2="1.5cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="15.0cm" Y1="2.0cm" X2="16.0cm" Y2="2.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="15.0cm" Y1="2.5cm" X2="16.0cm" Y2="2.5cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="0.0cm" Y1="3.0cm" X2="16.0cm" Y2="3.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="0.0cm" Y1="3.5cm" X2="16.0cm" Y2="3.5cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="0.0cm" Y1="4.0cm" X2="16.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockBorderLineStyle}"/>
            <!-- vertical lines -->
            <Line X1="0.0cm" Y1="0.0cm" X2="0.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockBorderLineStyle}"/>
            <Line X1="1.0cm" Y1="0.0cm" X2="1.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="2.0cm" Y1="0.0cm" X2="2.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="2.0cm" Y1="3.0cm" X2="2.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="3.0cm" Y1="0.0cm" X2="3.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="3.0cm" Y1="3.0cm" X2="3.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="4.0cm" Y1="0.0cm" X2="4.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="4.0cm" Y1="3.0cm" X2="4.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="5.0cm" Y1="0.0cm" X2="5.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="6.0cm" Y1="0.0cm" X2="6.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="6.0cm" Y1="3.0cm" X2="6.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="7.0cm" Y1="0.0cm" X2="7.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="7.0cm" Y1="3.0cm" X2="7.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="8.0cm" Y1="0.0cm" X2="8.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="8.0cm" Y1="3.0cm" X2="8.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="9.0cm" Y1="0.0cm" X2="9.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="9.0cm" Y1="3.0cm" X2="9.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="10.0cm" Y1="0.0cm" X2="10.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="10.0cm" Y1="3.0cm" X2="10.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="11.0cm" Y1="0.0cm" X2="11.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="12.0cm" Y1="0.0cm" X2="12.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="12.0cm" Y1="3.0cm" X2="12.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="13.0cm" Y1="0.0cm" X2="13.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="13.0cm" Y1="3.0cm" X2="13.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="14.0cm" Y1="0.0cm" X2="14.0cm" Y2="1.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="14.0cm" Y1="3.0cm" X2="14.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="15.0cm" Y1="0.0cm" X2="15.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockLineStyle}"/>
            <Line X1="16.0cm" Y1="0.0cm" X2="16.0cm" Y2="4.0cm" Style="{StaticResource ResourceKey=ControlBlockBorderLineStyle}"/>
        </Canvas>
    </ControlTemplate>

    <!-- 01: one-way motor control block -->

    <ControlTemplate x:Key="OneWayMotorControlBlock">
        <Canvas Width="16.0cm" Height="4.0cm" ClipToBounds="False" Style="{StaticResource ResourceKey=ControlBlockBaseCanvasStyle}">
            <!-- control block base template -->
            <Control Template="{StaticResource ResourceKey=ControlBlockBaseTemplate}"/>
            <!-- control block title text -->
            <Grid Width="14.0cm" Height="1.0cm" Canvas.Left="1.0cm" Canvas.Top="1.0cm">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="4.0cm"/>
                    <ColumnDefinition Width="6.0cm"/>
                    <ColumnDefinition Width="4.0cm"/>
                </Grid.ColumnDefinitions>
                <TextBlock Text="OFF" Grid.Column="0" Style="{StaticResource ResourceKey=ControlBlockTitleTextStyle}"/>
                <TextBlock Text="MOTOR" Grid.Column="1" Style="{StaticResource ResourceKey=ControlBlockTitleTextStyle}"/>
                <TextBlock Text="ON" Grid.Column="2" Style="{StaticResource ResourceKey=ControlBlockTitleTextStyle}"/>
            </Grid>
            <!-- control block input text -->
            <Grid Width="16.0cm" Height="1.0cm" Canvas.Left="0.0cm" Canvas.Top="0.0cm">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.5cm"/>
                    <RowDefinition Height="0.5cm"/>
                </Grid.RowDefinitions>
                <TextBlock Text="SA" ToolTip="Protection Off" Grid.Column="1" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="YB32" ToolTip="PROT OFF" Grid.Column="1" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="FA" ToolTip="Release Off" Grid.Column="2" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="YB34" ToolTip="REL OFF" Grid.Column="2" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="HA" ToolTip="Command Hand Off" Grid.Column="3" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="YB12" ToolTip="HND OFF" Grid.Column="3" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="AA" ToolTip="Command Auto Off" Grid.Column="4" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="YB22" ToolTip="AUTO OFF" Grid.Column="4" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="ST" ToolTip="Fault" Grid.Column="7" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="YB44" ToolTip="FAULT" Grid.Column="7" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="AE" ToolTip="Command Auto On" Grid.Column="11" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="YB21" ToolTip="AUTO ON" Grid.Column="11" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="HE" ToolTip="Command Hand On" Grid.Column="12" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="YB11" ToolTip="HND ON" Grid.Column="12" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="FE" ToolTip="Release On" Grid.Column="13" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="YB33" ToolTip="REL ON" Grid.Column="13" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="SE" ToolTip="Protection On" Grid.Column="14" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="YB31" ToolTip="PROT ON" Grid.Column="14" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
            </Grid>
            <!-- control block output text -->
            <Grid Width="16.0cm" Height="1.0cm" Canvas.Left="0.0cm" Canvas.Top="3.0cm">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                    <ColumnDefinition Width="1.0cm"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.5cm"/>
                    <RowDefinition Height="0.5cm"/>
                </Grid.RowDefinitions>
                <TextBlock Text="XB02" ToolTip="Fb OFF" Grid.Column="2" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="REA" ToolTip="Feedback Off" Grid.Column="2" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="XB92" ToolTip="Cmd OFF" Grid.Column="3" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="BA" ToolTip="Command Off" Grid.Column="3" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="XB46" ToolTip="Fb FAULT" Grid.Column="7" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="RST" ToolTip="Feedback Fault" Grid.Column="7" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="XB01" ToolTip="Fb ON" Grid.Column="12" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="REE" ToolTip="Feedback On" Grid.Column="12" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="XB91" ToolTip="Cmd ON" Grid.Column="13" Grid.Row="0" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
                <TextBlock Text="BE" ToolTip="Command On" Grid.Column="13" Grid.Row="1" Style="{StaticResource ResourceKey=ControlBlockInputTextStyle}"/>
            </Grid>
        </Canvas>
    </ControlTemplate>

